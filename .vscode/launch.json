  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "run tester",
      "program": "${workspaceFolder}\\src\\tester.ts",
      "outFiles": [
        "${workspaceFolder}/lib/**/*.js"
      ],
      "preLaunchTask": "npm: build"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "launch hello.ts",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\tester.ts",
      "preLaunchTask": "npm: build",
      "outFiles": [
        "c:\\github\\sr_git\\lib\\tester.js"
      ]
    },

  ]
